<application>
  <component name="RemoteServers">
    <remote-server name="Docker" type="docker">
      <configuration>
        <option name="apiUrl" value="tcp://localhost:2376" />
        <option name="pathMappings">
          <list>
            <DockerPathMappingImpl>
              <option name="localPath" value="$USER_HOME$/PhpstormProjects/crm" />
              <option name="virtualMachinePath" value="/CRM" />
            </DockerPathMappingImpl>
            <DockerPathMappingImpl>
              <option name="localPath" value="$USER_HOME$/PhpstormProjects/crm/core" />
              <option name="virtualMachinePath" value="/core" />
            </DockerPathMappingImpl>
          </list>
        </option>
      </configuration>
    </remote-server>
    <remote-server name="Docker test" type="docker">
      <configuration>
        <option name="apiUrl" value="unix:///var/run/docker.sock" />
        <option name="certificatesPath" />
        <option name="pathMappings">
          <list>
            <DockerPathMappingImpl>
              <option name="localPath" value="$USER_HOME$/PhpstormProjects/crm" />
              <option name="virtualMachinePath" value="/CRM" />
            </DockerPathMappingImpl>
          </list>
        </option>
      </configuration>
    </remote-server>
    <remote-server name="Docker3" type="docker">
      <configuration>
        <option name="apiUrl" value="unix:///var/run/docker.sock" />
        <option name="certificatesPath" />
        <option name="pathMappings">
          <list />
        </option>
      </configuration>
    </remote-server>
  </component>
</application>