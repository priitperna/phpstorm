<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Back">
        <Item>
          <IdeaActionID>Back</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Left' or 'Button4 Click'</ShortCut>
          <Description>Back</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B' or 'Ctrl+Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>82</Count>
        </Item>
      </Statistic>
      <Statistic Action="Find Action...">
        <Item>
          <IdeaActionID>GotoAction</IdeaActionID>
          <ShortCut>'Ctrl+Shift+A'</ShortCut>
          <Description>Find Action...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Next Page">
        <Item>
          <IdeaActionID>Console.TableResult.NextPage</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Down'</ShortCut>
          <Description>Next Page</Description>
          <Count>10</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'Alt+1'</ShortCut>
          <Description>Project</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Symbol...">
        <Item>
          <IdeaActionID>GotoSymbol</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+N'</ShortCut>
          <Description>Symbol...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'Alt+F12'</ShortCut>
          <Description>Terminal</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'Ctrl+F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>9</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>